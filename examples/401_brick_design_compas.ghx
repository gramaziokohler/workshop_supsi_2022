                    <item name="CodeInput" type_name="gh_string" type_code="10">from compas.datastructures import Mesh
from compas.colors import Color
from compas.geometry import Box
import compas_ghpython

def brick(plane):
    """ List of points defining the corners of the brick 
    
    Returns
    ----------
    [pt0 : bottom point at negative x and negative y,
    pt1 : bottom point at negative x and possitive y,
    pt2 : bottom point at possitive x and possitve y,
    pt3 : bottom point at possitive x and negative y,
    pt4 : pt0 + Thickness,
    pt5 : pt1 + Thickness,
    pt6 : pt2 + Thickness,
    pt7 : pt3 + Thickness]
    """
    
    LENGTH = 12.5 
    WIDTH = 6
    HEIGHT = 4
    
    origin = plane.point
    
    x_vec = plane.xaxis
    y_vec = plane.yaxis 
    z_vec = plane.normal

    pt_0 = origin - (x_vec *LENGTH/2) - (y_vec*WIDTH/2)
    pt_1 = origin + (x_vec *LENGTH/2) - (y_vec*WIDTH/2)
    pt_2 = origin + (x_vec *LENGTH/2) + (y_vec*WIDTH/2)
    pt_3 = origin - (x_vec *LENGTH/2) + (y_vec*WIDTH/2)
    
    pt_4 = pt_0 + (z_vec*HEIGHT)
    pt_5 = pt_1 + (z_vec*HEIGHT)
    pt_6 = pt_2 + (z_vec*HEIGHT)
    pt_7 = pt_3 + (z_vec*HEIGHT)
    
    pts = [pt_0, pt_1, pt_2, pt_3, pt_4, pt_5, pt_6, pt_7]
    
    
    
    return pts

    
def mesh(m_pts):
    """Mesh  depicting the brick:

    Returns
    ----------
    mesh_brick : Mesh
    """
    box = Box.from_bounding_box(m_pts)
    mesh_brick = Mesh.from_shape(box)
    return mesh_brick


brick_meshes = []
brick_frames = positions

for pos in positions:
    brick_pts = brick(pos)
    brick_mesh = mesh(brick_pts)

    vertices, faces = brick_mesh.to_vertices_and_faces()
    brick_meshes.append(compas_ghpython.draw_mesh(vertices, faces, color=Color(.0, .1, .1, .5).rgba255))
