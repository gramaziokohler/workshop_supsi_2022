                    <item name="CodeInput" type_name="gh_string" type_code="10">from compas.geometry import Frame, Point, Rotation
import math as m



"""Generates a COMPAS frames for each brick

This is where the design of the wall is created.

Returns
----------
brick_planes : [COMPAS Frame, COMPAS Frame, ...]

"""


VERTICAL_COUNT = 20
HORIZONTAL_COUNT = 10


BRICK_LENGTH = 12.5
BRICK_HEIGHT = 4

brick_planes = []

for i in range (VERTICAL_COUNT):
    for j in range (HORIZONTAL_COUNT):
        if i%2 ==0:
            x_pos = j * (BRICK_LENGTH + 1)
            rot = m.radians(10)
            
        else:
            x_pos = j * (BRICK_LENGTH + 1) + BRICK_LENGTH/2
            rot = m.radians(-10)
            
        z_pos = i * BRICK_HEIGHT
        
        origin = Point(x_pos, 0, z_pos)
        
        frame = Frame(origin, (1, 0, 0), (0, 1, 0))
        frame.transform(Rotation.from_axis_and_angle(frame.normal, rot, frame.point))
        brick_planes.append(frame)
